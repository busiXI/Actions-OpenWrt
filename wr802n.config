# Target配置
CONFIG_TARGET_ath79=y
CONFIG_TARGET_ath79_generic=y
CONFIG_TARGET_ath79_generic_DEVICE_tplink_tl-wr802n-v3=y

# 镜像优化配置
CONFIG_TARGET_ROOTFS_PARTSIZE=51200
CONFIG_TARGET_KERNEL_PARTSIZE=16
CONFIG_TARGET_OPTIMIZATION="-Os -pipe -mno-branch-likely -mips32r2 -mtune=24kc"
CONFIG_KERNEL_BUILD_USER="OptimizedBuild"

# 内存关键优化
CONFIG_PACKAGE_luci=n
CONFIG_PACKAGE_luci-ssl=n
CONFIG_IPV6=n
CONFIG_KERNEL_DEBUG_FS=n
CONFIG_KERNEL_DEBUG_KERNEL=n

# Tailscale精简配置
CONFIG_PACKAGE_tailscale=y
CONFIG_PACKAGE_tailscaled=y
CONFIG_PACKAGE_tailscale-nginx=n  # 禁用Web集成

# 网络核心配置
CONFIG_PACKAGE_kmod-tun=y
CONFIG_PACKAGE_iptables-mod-extra=y
CONFIG_PACKAGE_libustream-openssl=y

# 系统精简配置
CONFIG_PACKAGE_ppp=n
CONFIG_PACKAGE_ppp-mod-pppoe=n
CONFIG_PACKAGE_dnsmasq-full=y
