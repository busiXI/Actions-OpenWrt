# Target 配置
CONFIG_TARGET_ath79=y
CONFIG_TARGET_ath79_generic=y
CONFIG_TARGET_ath79_generic_DEVICE_tplink_tl-wr802n-v1=y

# 分区配置
CONFIG_TARGET_ROOTFS_PARTSIZE=61440  # 60MB (0x3c00000)
CONFIG_TARGET_KERNEL_PARTSIZE=16      # 16MB

# 基础组件
CONFIG_PACKAGE_block-mount=y
CONFIG_PACKAGE_iptables-mod-extra=y
CONFIG_PACKAGE_kmod-ath9k=y

# Tailscale
CONFIG_PACKAGE_tailscale=y
CONFIG_PACKAGE_tailscaled=y

# 优化配置
CONFIG_PACKAGE_luci=n
CONFIG_IPV6=n
CONFIG_KERNEL_DEBUG_FS=n
